.nearbyindex .box {
  width: 95%;
  margin: 0 auto;
}
.nearbyindex .box .search {
  width: 100%;
  height: 80rpx;
  background-color: #fff;
}
.nearbyindex .box .change {
  margin-top: 2%;
  padding-bottom: 3%;
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  overflow-y: auto;
  -webkit-box-shadow: 0 0px 1px rgba(0, 0, 0, 0.3), 0 0 3px rgba(0, 0, 0, 0.1) inset;
          box-shadow: 0 0px 1px rgba(0, 0, 0, 0.3), 0 0 3px rgba(0, 0, 0, 0.1) inset;
  -webkit-border-radius: 8rpx;
          border-radius: 8rpx;
}
.nearbyindex .box .change .change-item {
  padding-left: 2%;
  padding-right: 2%;
  margin-left: 2%;
  height: 70rpx;
  line-height: 70rpx;
  border: 1px solid #d6d6d6;
  text-align: center;
  color: rgba(0, 0, 0, 0.447059);
  -webkit-border-radius: 8rpx;
          border-radius: 8rpx;
  margin-top: 2%;
  margin-bottom: 1%;
  white-space: normal;
  overflow: hidden;
  text-overflow: ellipsis;
}
.nearbyindex .box .change .active {
  border: 1px solid #7b92fa;
  color: #7b92fa;
}
.nearbyindex .box .changeHeight {
  height: auto;
  -webkit-animation-name: example;
          animation-name: example;
  -webkit-animation-duration: 0.6s;
          animation-duration: 0.6s;
  -webkit-animation-fill-mode: forwards;
          animation-fill-mode: forwards;
}
@-webkit-keyframes example {
0% {
    opacity: 0;
}
15% {
    opacity: 0.1;
}
30% {
    opacity: 0.2;
}
45% {
    opacity: 0.3;
}
60% {
    opacity: 0.5;
}
75% {
    opacity: 0.6;
}
90% {
    opacity: 0.8;
}
100% {
    opacity: 1;
}
}
@keyframes example {
