<view class="nearbydetail"><view class="title"><view class="title-left _div"><view class="_div">{{parkDetail.park_name}}</view><view class="_div"><label class="_span">{{parkDetail.distance+"米"}}</label><label class="_span">|</label><label class="_span">总车位</label><label class="_span">{{parkDetail.park_num}}</label></view></view><view data-event-opts="{{[['tap',[['call',['$event']]]]]}}" class="title-right _div" bindtap="__e"><view class="_div"><image class="phoneImg" src="{{phoneImg}}" mode></image></view></view></view><view class="photo"><swiper class="swiper" indicator-dots="true" indicator-color="#d6d6d6" indicator-active-color="#fff" autoplay="true" circular="true"><block wx:for="{{parkDetail.image}}" wx:for-item="item" wx:for-index="index"><swiper-item><image src="{{item.url}}" mode></image></swiper-item></block></swiper></view><view class="info"><view class="info-item _div"><view class="info-item-img _div"><image src="{{parkImg}}" mode></image></view><view class="_div">{{"空余车位："+parkDetail.surplus_num}}</view></view><view class="info-item _div"><view class="info-item-img _div"><image src="{{timeImg}}" mode></image></view><view class="_div">{{"营业时间："+parkDetail.business_hours_start+"-"+parkDetail.business_hours_end}}</view></view><view class="info-item _div"><view class="info-item-img _div"><image src="{{dwImg}}" mode></image></view><view style="width:70%;white-space:normal;overflow:hidden;text-overflow:ellipsis;" class="_div">{{''+parkDetail.detail_address+''}}</view><view data-event-opts="{{[['tap',[['goMap',['$event']]]]]}}" bindtap="__e" class="_div">前往</view></view></view><view class="rules"><view class="_div">收费规则</view><view class="_div"><label class="_span">{{parkDetail.charges+"元"}}</label><label class="_span">/小时</label></view></view></view>