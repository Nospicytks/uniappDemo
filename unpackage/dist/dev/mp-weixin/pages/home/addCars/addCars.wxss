.addCars .head {
  width: 60%;
  margin: 0 auto;
  height: 80rpx;
  background-color: #fff;
  -webkit-border-radius: 49rpx;
          border-radius: 49rpx;
  margin-top: 10%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding-left: 2%;
  text-align: center;
}
.addCars .head .head-item {
  width: 46%;
  -webkit-border-radius: 49rpx;
          border-radius: 49rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 10rpx;
  font-size: 30rpx;
  color: rgba(0, 0, 0, 0.447059);
  cursor: pointer;
}
.addCars .head .head-item:nth-child(2) {
  margin-left: 4%;
}
.addCars .head .ative {
  background-color: #6b86f9;
  color: #fff;
}
.addCars .num {
  width: 90%;
  height: 100rpx;
  margin: 0 auto;
  margin-top: 20%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.addCars .num .num-item {
  width: 9%;
  margin-left: 1%;
  border: 1px solid #d6d6d6;
  -webkit-border-radius: 8rpx;
          border-radius: 8rpx;
  line-height: 100rpx;
  text-align: center;
}
.addCars .num .num-item:nth-child(3) {
  border: none;
}
.addCars .add {
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  background-color: #4d74ff;
  color: #fff;
  margin: 0 auto;
  font-size: 30rpx;
  font-weight: 600;
  margin-top: 20%;
}
.addCars .history {
  width: 90%;
  margin: 0 auto;
  margin-top: 10%;
}
.addCars .history ._div:nth-child(1) {
  font-weight: bold;
  letter-spacing: 2px;
}
.addCars .history .history-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.addCars .history .history-item {
  width: 32%;
  height: 55rpx;
  line-height: 55rpx;
  background-color: #d0def9;
  -webkit-border-radius: 30rpx;
          border-radius: 30rpx;
  color: #3a89ff;
  font-size: 21rpx;
  margin-left: 1.2%;
  margin-top: 3%;
}
.addCars .history .history-item ._div:nth-child(1) {
  text-align: center;
  white-space: nowrap;
}
.addCars .history .history-item ._div:nth-child(2) {
  width: 17%;
  height: 35rpx;
  line-height: 35rpx;
  background-color: #fff;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  margin-top: 10rpx;
  text-align: center;
  font-size: 20rpx;
  color: rgba(0, 0, 0, 0.447059);
  padding-left: 1.5%;
}

